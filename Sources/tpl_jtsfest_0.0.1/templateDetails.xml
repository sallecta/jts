		<language tag="en-GB">en-GB/tpl_jstfest.sys.ini</language>
	</languages>
-->
	<config>
		<fields name="params">
			<fieldset name="advanced" addfieldpath="/templates/jstfest/fields">

				<field name="jstfest_params" type="subformjstfest"><form/></field>
				
			</fieldset>
