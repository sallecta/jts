<?xml version="1.0" encoding="utf-8"?>
<extension type="template" client="site">
	<name>jtsfest</name>
	<version>0.0.1</version>
	<creationDate>October 2022</creationDate>
	<author>Alexander Gribkov, Joomla! Project</author>
	<authorEmail>sallecta@inbox.ru</authorEmail>
	<copyright>(C) 2022 Open Source Matters, Inc.</copyright>
	<description>JTSFEST_DESCR</description>
	<inheritable>1</inheritable>
	<files>
		<filename>component.php</filename>
		<filename>error.php</filename>
		<filename>index.php</filename>
		<filename>offline.php</filename>
		<filename>templateDetails.xml</filename>
		<folder>html</folder>
		<folder>subthemes</folder>
		<folder>code</folder>
		<folder>config_shared</folder>
		<folder>fields</folder>
		<folder>language</folder>
	</files>
	<positions>
		<position>topbar</position>
		<position>below-top</position>
		<position>menu</position>
		<position>search</position>
		<position>banner</position>
		<position>top-a</position>
		<position>top-b</position>
		<position>main-top</position>
		<position>main-bottom</position>
		<position>breadcrumbs</position>
		<position>sidebar-left</position>
		<position>sidebar-right</position>
		<position>bottom-a</position>
		<position>bottom-b</position>
		<position>footer</position>
		<position>debug</position>
	</positions>
	<config>
		<fields name="params">
			<fieldset name="advanced" addfieldpath="/templates/jtsfest/">

				<field name="jtsfest_params" type="subformjts"><form/></field>
				
			</fieldset>
		</fields>
	</config>
	<changelogurl>https://raw.githubusercontent.com/sallecta/jtsfest/main/Updates/change_log.xml</changelogurl>
	<updateservers>
		<server type="extension" priority="1" name="jtsfest update server">https://raw.githubusercontent.com/sallecta/jtsfest/main/Updates/updt.xml</server>
	</updateservers>
</extension>
